{
  "name": "ynab-mcp-server",
  "version": "0.1.0",
  "description": "Model Context Protocol server for interacting with the YNAB API.",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "generate:types": "python tools/generate_types.py",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "dotenv": "^16.4.5",
    "undici": "^5.28.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.9"
  }
}
